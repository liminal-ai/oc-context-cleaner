{
  "name": "oc-context-cleaner",
  "version": "0.1.0",
  "description": "CLI tool for cleaning OpenClaw session transcripts",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "occ": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write src tests",
    "format:check": "biome format src tests",
    "lint": "biome lint src tests",
    "check": "biome check src tests",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "openclaw",
    "context",
    "cli",
    "session"
  ],
  "license": "MIT",
  "dependencies": {
    "c12": "^3.3.3",
    "citty": "^0.2.0",
    "pathe": "^2.0.3",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@types/node": "^22.12.0",
    "memfs": "^4.56.10",
    "typescript": "^5.9.3",
    "vitest": "^3.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
